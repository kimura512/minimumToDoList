{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["DOM", "DOM.Iterable", "ES2020"],
      "jsx": "react-jsx",
      "module": "ESNext",
      "moduleResolution": "node",
      "strict": true,
      "noEmit": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true
    },
    "include": ["src"]
  }
  